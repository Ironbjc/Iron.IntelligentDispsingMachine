<?xml version="1.0" encoding="utf-8" ?>
<Config>
  <Connstr>
  <!--药房数据库连接字符串-->
  <!-- <DBName>昆山西部门诊西药</DBName> -->
  <DBName>Hospital_Pharmacy_sss</DBName>
  <DBIP>.</DBIP>
  <USERID>sa</USERID>
  <!-- <PWD>iron123!</PWD> -->
  <PWD>123456</PWD>
  <!--设置药柜ID号-->
  <MachineID>01</MachineID>
  <DPortCom>COM21</DPortCom>
  <!--设置柜体层数八层或者九层-->
  <!--设置系统调试码-->
  <ConfirmCode>8888</ConfirmCode>
  <!--设置伺服电机端口-->
  <ServoPortCom>COM8</ServoPortCom>
  <!--设置系统射灯端口-->
  <DPortCom>COM21</DPortCom>
  <ArkLayer>九层</ArkLayer>
