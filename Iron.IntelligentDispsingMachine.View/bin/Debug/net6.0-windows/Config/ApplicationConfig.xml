  <!--电机自动运行速度-->
  <MotorAutoSpeed>8000</MotorAutoSpeed>
  <!--电机手动运行高速度-->
  <MotorHandSpeed>1000</MotorHandSpeed>
  <!--电机回原点速度-->
  <MotorZeroSpeed>1500</MotorZeroSpeed>
  <!--出药模式，0代表扫码出库-->
  <MedOutMode>1</MedOutMode>
  <ScanCom>COM5</ScanCom>
